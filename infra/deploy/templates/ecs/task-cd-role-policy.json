{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "iam:CreateRole",
          "iam:CreatePolicy",
          "iam:AttachRolePolicy",
          "logs:CreateLogGroup",
          "logs:ListTagsForResource",
          "ecs:CreateCluster",
          "rds:CreateDBSubnetGroup"
        ],
        "Resource": "*"
      }
    ]
  }